<section class="info_section layout_padding" id="contact-info">
    <div class="container">
      <div class="row">
        <div class="col-md-3">
          <div class="info_logo me-3">
            <div>
              <a href="">
                <!-- <img src="images/logo.png" alt="" /> -->
                <img src="./img/logo.png" alt="Logo" class="logo_image" />
              </a>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="info_links">
            <h5>Contact Us</h5>
            <p class="pr-0 pr-md-4 pr-lg-5"> <a href="tel:+91<%= phoneNumber %>" class="remove-hyp-color">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-telephone-fill" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M1.885.511a1.745 1.745 0 0 1 2.61.163L6.29 2.98c.329.423.445.974.315 1.494l-.547 2.19a.68.68 0 0 0 .178.643l2.457 2.457a.68.68 0 0 0 .644.178l2.189-.547a1.75 1.75 0 0 1 1.494.315l2.306 1.794c.829.645.905 1.87.163 2.611l-1.034 1.034c-.74.74-1.846 1.065-2.877.702a18.6 18.6 0 0 1-7.01-4.42 18.6 18.6 0 0 1-4.42-7.009c-.362-1.03-.037-2.137.703-2.877z"/>
            </svg> <%= phoneNumber %> </a></p>
            <p class="pr-0 pr-md-4 pr-lg-5"><a href="mailto:<%= emailId %>" class="remove-hyp-color">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-envelope-fill" viewBox="0 0 16 16">
                <path d="M.05 3.555A2 2 0 0 1 2 2h12a2 2 0 0 1 1.95 1.555L8 8.414zM0 4.697v7.104l5.803-3.558zM6.761 8.83l-6.57 4.027A2 2 0 0 0 2 14h12a2 2 0 0 0 1.808-1.144l-6.57-4.027L8 9.586zm3.436-.586L16 11.801V4.697z"/>
              </svg>
              <%= emailId %></a></p>
            <p class="pr-0 pr-md-4 pr-lg-5"><a href="<%= facebookLink %>" class="remove-hyp-color">
              <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-facebook" viewBox="0 0 16 16">
                <path d="M16 8.049c0-4.446-3.582-8.05-8-8.05C3.58 0-.002 3.603-.002 8.05c0 4.017 2.926 7.347 6.75 7.951v-5.625h-2.03V8.05H6.75V6.275c0-2.017 1.195-3.131 3.022-3.131.876 0 1.791.157 1.791.157v1.98h-1.009c-.993 0-1.303.621-1.303 1.258v1.51h2.218l-.354 2.326H9.25V16c3.824-.604 6.75-3.934 6.75-7.951"/>
              </svg> Facebook</a></p>
          </div>
        </div>
        <div class="col-md-4">
          <div class="info_insta">
            <h5>Branches</h5>
            <% if (Array.isArray(branchAddress)) { %>
              <%for (let i = 0; i < branchAddress.length; i++) {%>
                <p class="pr-0 pr-md-4 pr-md-5"><b>Branch<%= i + 1 %>:</b> <%= branchAddress[i] %> </p>
              <% } %>
            <% } else { %>
              <p class="pr-0 pr-md-4 pr-md-5"><b>Branch:</b> <%= branchAddress %> </p>
            <% } %>
          </div>
        </div>
        <!-- <div class="col-md-3">
          <div class="pl-0 pl-lg-5 pl-md-4 info_account">
            <h5>Compliance</h5>
              <a href="" class="remove-hyp-color">Accessibility</a><br>
              <a href="" class="remove-hyp-color">Privacy</a><br>
              <a href="" class="remove-hyp-color">Terms Of Use</a><br>
          </div>
        </div> -->
      </div>
    </div>
</section>
